{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prop-types": 0,
        "max-len": 0,
        "no-underscore-dangle": ["error", {
            "allow": ["__REDUX_DEVTOOLS_EXTENSION_COMPOSE__"]
        }],
        "jsx-a11y/no-noninteractive-element-interactions": 0,
        "react/no-unused-state": 0
    },
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    }
}